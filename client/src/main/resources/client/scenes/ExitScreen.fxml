<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>

<AnchorPane prefHeight="571.0" prefWidth="961.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.scenes.ExitScreenCtrl">
   <children>
      <Label layoutY="139.0" prefHeight="17.0" prefWidth="961.0" text="Are you sure you want  to quit?" textAlignment="CENTER" />
      <StackPane layoutY="248.0" prefHeight="150.0" prefWidth="961.0">
         <children>
            <Button fx:id="quitButton" mnemonicParsing="false" onMouseClicked="#quit" text="Yes">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
               <StackPane.margin>
                  <Insets right="300.0" />
               </StackPane.margin>
            </Button>
            <Button fx:id="closeButton" cancelButton="true" mnemonicParsing="false" onMouseClicked="#closeButtonMouseClicked" text="No">
               <StackPane.margin>
                  <Insets left="300.0" />
               </StackPane.margin>
            </Button>
         </children>
      </StackPane>
   </children>
</AnchorPane>
